{
"update_url": "https://clients2.google.com/service/update2/crx",

    "name": "iTry Message Sender",
    "version": "1.0",
    "description": "WhatsHelper",
    "permissions": [
        "tabs",
        "storage",
        "activeTab", 
        "notifications",
        "scripting",
        "sidePanel"
    ],
    "host_permissions": [
        "http://*/*",
        "https://*/*"
    ],
    "background": {
        "service_worker": "background.js"
    },
    "action": {
        "default_title": "WhatsHelper"
    },
    "side_panel": {
        "default_path": "popup.html"
    },
    "content_scripts": [{
        "matches": ["<all_urls>"],
        "js": [
            "content.js"
        ]
    }],
    "icons": {
        "16": "images/16.png",
        "32": "images/32.png",
        "48": "images/48.png"
    },
    "manifest_version": 3
    
}